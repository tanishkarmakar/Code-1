//FUNCTION 1
//this function takes an argument and returns that value plus 7.
int addFive(int n) {
  return n + 7;
}

//FUNCTION 2
//this function takes an argument and returns that value minus 14.
int subtractTen(int n) {
  return n - 14;
}

//FUNCTION 3
//this function takes two arguments and returns the result of their addition
int addTwoInts(int n, int x) {
  return n + x;
}

//FUNCTION 4
//this function takes two arguments and returns the result of their division
int divideTwoInts(int n, int x) {
  return n/x;
}
//FUNCTION 5
//this function takes two arguments and returns the result of their multiplication
int multiplyTwoInts(int n, int x) {
  return n*x;
}

